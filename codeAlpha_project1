pragma solidity ^0.8.0;

contract SimpleStorage{
    int public value=0; //variable to store number 

    //increment function 
    function increment()public{
        value= value+1;

    }

     // decrement function 
     function decrement () public {
            value = value - 1;  
        }

        function getValue() public view returns (int){
            return value;
        }
}
